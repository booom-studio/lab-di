  },
  "devDependencies": {
    "babel-cli": "6.24.0",
    "babel-core": "6.24.0",
    "babel-preset-es2015": "6.24.0",
    "cucumber": "1.2.1",
    "npm-watch": "0.1.6"
  }
}
